# Production Environment Variables for Dockploy
# Copy this file to .env in your server

# Database Configuration
POSTGRES_DB=dishdash
POSTGRES_USER=dishdash_user
POSTGRES_PASSWORD=your_secure_password_here
DATABASE_URL=postgresql://dishdash_user:your_secure_password_here@db:5432/dishdash

# Environment
ENVIRONMENT=production

# Security
SECRET_KEY=your_secret_key_here_minimum_32_characters
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# GitHub Container Registry
GITHUB_USERNAME=hugobrun343

# Mistral AI API Key
MISTRAL_API_KEY=your_mistral_api_key_here

# Frontend Configuration
NEXT_PUBLIC_API_URL=https://api-dishdash.hugobrun.fr/api/v1
NODE_ENV=production

